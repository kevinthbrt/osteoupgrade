# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=votre_url_supabase
NEXT_PUBLIC_SUPABASE_ANON_KEY=votre_cle_anon_supabase

# Emailing
RESEND_API_KEY=votre_cle_api_resend
# Doit correspondre à un domaine validé dans Resend (ex: "OsteoUpgrade <no-reply@osteo-upgrade.fr>")
RESEND_FROM="OsteoUpgrade <no-reply@osteo-upgrade.fr>"
NEXT_PUBLIC_RESEND_FROM="OsteoUpgrade <no-reply@osteo-upgrade.fr>"

# System.io Configuration
SYSTEMIO_API_KEY=votre_cle_api_systemio
SYSTEMIO_API_URL=https://systeme.io/api/v1

# Vercel Blob Storage (pour les uploads de fichiers)
# Cette variable est automatiquement générée par Vercel si vous activez Blob Storage
# Dashboard Vercel > Storage > Create > Blob > Le token sera automatiquement ajouté
BLOB_READ_WRITE_TOKEN=votre_token_vercel_blob

# Note: Ces variables doivent être configurées dans Vercel
# Dashboard Vercel > Settings > Environment Variables
